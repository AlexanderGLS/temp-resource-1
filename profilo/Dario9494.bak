<script>
	var miowidth = screen.width;
       $(document).ready(function () {
		   
		if(miowidth <= 1366){
			$(".bachecaprofilo").removeClass("col-xl-8");
			$(".bachecaprofilo").addClass("col-xl-12");
			$(".sidebarProfilo").addClass("col-xl-12");
		  }
        		  var giorno = new Date();
		  var ora = giorno.getHours();
		  $("#customRange1").val(ora);
		  $("#provino").text(ora);
		  cambiaSfondoOra(ora);
		    

      $('#manoToggle').on('click', function () {
      $('#sidebar').toggleClass('active');
      $('#content').toggleClass('largo');
      $(this).toggleClass('manoAperta');
      });
        
		$("#lightgallery").lightGallery({
				selector: '.col-lg-3'
			});
      });
	  
      $(document).on("click",".wrapActionCommentiPost",function(){
		var postid = $(this).data("postid");
		$(".wrapCommentaIlPost[data-postid="+postid+"]").toggleClass("attivaCommentaPost");
	  });
	  
	  $(document).on("click",".wrapCommentiPost",function(){
		var postid = $(this).data("postid");
		$(".listaCommenti[data-postid="+postid+"]").toggleClass("attivaListaCommenti");
	  });
	  
	
	  
	  $(document).on("click",".wrapActionLikePost",function(){
		var postid = $(this).data("postid");		
		var post = $(this);
		var counter = $(".wrapLikePost[data-postid="+postid+"] > span").text();
		$.ajax({
			type: "POST",
			url: "/ajax/likepost.php",
			data: { postid:postid},
			success:function(result){
				if(result == 1)
				{
					$(post).find("span").text("Non mi piace");
					counter++;
          $(post).find("i").removeClass("icon-like");
          $(post).find("i").addClass("icon-unlike");
					$(".wrapLikePost[data-postid="+postid+"] > span").text(counter);
				}else if(result == 2)
				{
					Swal.fire({
					  title: 'Attenzione!',
					  text: 'Si √® verificato un errore durante l\'azione',
					  icon: 'error',
					  confirmButtonText: 'Ok!'
					})
				}else if(result == 3)
				{
					$(post).find("span").text("Mi Piace");
					counter--;
          $(post).find("i").removeClass("icon-unlike");
          $(post).find("i").addClass("icon-like");
					$(".wrapLikePost[data-postid="+postid+"] > span").text(counter);
				}else if(result == 4)
				{
					Swal.fire({
					  title: 'Attenzione!',
					  text: 'Per usare la parte social devi verificare il tuo account, vai su impostazioni e clicca su \'Verifica adesso\'',
					  icon: 'error',
					  confirmButtonText: 'Ok!'
					})
					$(tempo).removeAttr("disabled");
				}		
			}
		});
		
		$(this).toggleClass("mipiacePost");
	  });
	  
	 $(document).on('keyup', '.postoCommento', function(e){
			if(e.keyCode == 13)
			{
			  var id = $(this).data('postid');
				var testo = $(this).val();
				var tempo = $(this);
				$(this).attr("disabled", "disabled");
				
				$(this).val('');
				$.ajax({
					type: "POST",
					url: "/ajax/makecommento.php",
					data: { id:id, testo:testo},
					success:function(result){
							
							if(result == 1)
							{
								$.ajax({
									url: '/ajax/provavistacom.php',
									data: {id:id},
									type: "POST",
									success: function(html) {
											$('.listaCommenti[data-postid='+id+']').html(html);
										} 
									});
								$(".listaCommenti[data-postid="+id+"]").toggleClass("attivaListaCommenti");
								$(tempo).removeAttr("disabled");
							}               
							else if(result == 2)
							{
								Swal.fire({
								  title: 'Attenzione!',
								  text: 'Scrivi qualcosa prima di cliccare il tasto Pubblica :P',
								  icon: 'warning',
								  confirmButtonText: 'Ok!'
								})
								$(tempo).removeAttr("disabled");
							}	
							else if(result == 3)
							{
								Swal.fire({
								  title: 'Attenzione!',
								  text: 'Per usare la parte social devi verificare il tuo account, vai su impostazioni e clicca su \'Verifica adesso\'',
								  icon: 'error',
								  confirmButtonText: 'Ok!'
								})
								$(tempo).removeAttr("disabled");
							}
				}});	
					}
			});
	 
	 
		$(document).on('click', '.caricaAltriCommenti', function(){
			var id = $(this).data('postid');
			$('.listaCommenti[data-postid='+id+']').html("CARICAMENTO");
				$(this).toggle();
			   $.ajax({
				type: 'POST',
				url: '/ajax/showallcomment.php',
				data:{id: id},
				success: function(data) {
				  $('.listaCommenti[data-postid='+id+']').html(data);
				},
				error:function(err){
				  alert("error"+JSON.stringify(err));
				}
			});
		 });
		
		
		$(document).on("click","#mostraDistintivi",function(){
