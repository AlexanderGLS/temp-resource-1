		$(document).on("click","#mostraFoto",function(){
			$('.bodyModalStoria').html("");
			var userid = 15;
			$.ajax({
				type: 'POST',
				url: '/ajax/getFotoProfile.php',
				data: {userid:userid},
				success: function(data) {
				  $('.bodyModalStoria').html(data);
				  $("#galleriaplus").lightGallery({
						selector: '.col-lg-2'
					});
				},
				error:function(err){
				  alert("error"+JSON.stringify(err));
				}
			});
			$(".wrapModalStoria").removeClass("esciStoria");
			$(".wrapModalStoria").addClass("entraStoria");
		})
		
	  $(document).on("click",".chiudiModalStoria",function(){
		$(".wrapModalStoria").removeClass("entraStoria");
		$(".wrapModalStoria").addClass("esciStoria");
	  })
